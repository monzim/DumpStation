# API Base URL - the backend server URL
# For local development
VITE_API_BASE_URL=http://localhost:8080/api/v1

# For production (set this in your Cloudflare dashboard or wrangler.jsonc)
# VITE_API_BASE_URL=https://your-api.example.com/api/v1

# Cloudflare Turnstile Site Key (Public Key for Frontend)
# Test key (always passes): 1x00000000000000000000AA
# Test key (always fails): 2x00000000000000000000AB
# Test key (forces interactive challenge): 3x00000000000000000000FF
# Get production keys from: https://dash.cloudflare.com/?to=/:account/turnstile
VITE_TURNSTILE_SITE_KEY=1x00000000000000000000AA
